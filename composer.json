{
    "name": "palpalani/magento2-rest-api-client",
    "description": "",
    "keywords": [
        "palPalani",
        "magento2",
        "rest-api"
    ],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "palPalani",
            "email": "palani.p@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.2|^8.0",
        "ext-curl": "*",
        "ext-json": "*"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.0",
        "phpunit/phpunit": "^9.5",
        "vimeo/psalm": "^4.8"
    },
    "autoload": {
        "psr-4": {
            "palPalani\\Magento2Api\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "palPalani\\Magento2Api\\Tests\\": "tests"
        }
    },
    "scripts": {
        "psalm": "vendor/bin/psalm",
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage",
        "format": "vendor/bin/php-cs-fixer fix --allow-risky=yes"
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
